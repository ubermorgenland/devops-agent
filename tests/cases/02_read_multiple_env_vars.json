{
  "id": "02",
  "name": "read_multiple_env_vars",
  "description": "Read multiple environment variables",
  "category": "env_vars",

  "setup": {
    "env_vars": {
      "DOCKER_USER": "testuser",
      "AWS_REGION": "us-east-1"
    },
    "files": {}
  },

  "instruction": "Get the values of DOCKER_USER and AWS_REGION environment variables",
  "max_steps": 4,

  "assertions": [
    {
      "type": "tool_call_count",
      "tool": "get_env",
      "count": 2
    },
    {
      "type": "completed_successfully"
    },
    {
      "type": "final_answer_contains",
      "values": ["testuser", "us-east-1"],
      "case_sensitive": false
    }
  ]
}
